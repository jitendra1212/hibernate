<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.nt.entity.Employee">
	<id name="eid" column="EID"  type="int" length="5">
	
	<!-- <generator class="increment">
	</generator> -->
	</id>
	<property name="firstName" column="FIRSTNAME" not-null="true" unique="true" type="string" length="15"></property>
	
	<property name="lastName" column="LASTNAME" not-null="true" unique="true" type="string" length="25"></property>
	<property name="email" column="EMAIL" not-null="true" unique="true"  type="string" length="15"></property>
	

	</class>
</hibernate-mapping>